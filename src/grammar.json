{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "scfg",
  "rules": {
    "config": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "directive"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^\\x00-\\x1F\\x7F\\x0A]"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "directive_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "directive_params"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "block"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "directive_name": {
      "type": "SYMBOL",
      "name": "word"
    },
    "directive_params": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "word"
          }
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_line"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "word": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_atom"
        },
        {
          "type": "SYMBOL",
          "name": "dquote_word"
        },
        {
          "type": "SYMBOL",
          "name": "squote_word"
        }
      ]
    },
    "_atom": {
      "type": "TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\x00-\\x1F\\x7F\"\\\\\\x0A'{}\\x20\\x09]"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\x00-\\x1F\\x7F\\x0A]"
                }
              ]
            }
          ]
        }
      }
    },
    "dquote_word": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\x00-\\x1F\\x7F\"\\\\\\x0A]"
              },
              {
                "type": "SYMBOL",
                "name": "esc_pair"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "squote_word": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^\\x00-\\x1F\\x7F'\\x0A]"
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "esc_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "PATTERN",
          "value": "[^\\x00-\\x1F\\x7F\\x0A]"
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\x0A"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}